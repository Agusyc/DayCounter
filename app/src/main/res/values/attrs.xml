<?xml version="1.0" encoding="UTF-8"?>
<resources>
    <declare-styleable name="ColorImageView">
        <attr name="picked_color" format="color" />
        <attr name="layout" format="reference" />
    </declare-styleable>

    <declare-styleable name="CustomTextInputLayout">
        <attr name="helperText" format="string" />
        <attr name="helperTextColor" format="color|reference" />
    </declare-styleable>

    <declare-styleable name="AutoGridLayout">
        <attr name="columnWidth" format="dimension" />
    </declare-styleable>

    <style name="HelperTextAppearance" parent="TextAppearance.Design.Error">
        <item name="android:textColor">#888888</item>
    </style>

    <declare-styleable name="MaterialSpinner">
        <attr name="ms_baseColor" format="integer"/>
        <attr name="ms_highlightColor" format="integer"/>
        <attr name="ms_errorColor" format="integer"/>
        <attr name="ms_error" format="string"/>
        <attr name="ms_hint" format="string"/>
        <attr name="ms_hintColor" format="color"/>
        <attr name="ms_hintTextSize" format="dimension"/>
        <attr name="ms_floatingLabelText" format="string"/>
        <attr name="ms_floatingLabelColor" format="color"/>
        <attr name="ms_multiline" format="boolean"/>
        <attr name="ms_nbErrorLines" format="integer"/>
        <attr name="ms_typeface" format="string"/>
        <attr name="ms_alignLabels" format="boolean"/>
        <attr name="ms_thickness" format="dimension"/>
        <attr name="ms_thickness_error" format="dimension"/>
        <attr name="ms_arrowSize" format="dimension"/>
        <attr name="ms_arrowColor" format="color"/>
        <attr name="ms_enableFloatingLabel" format="boolean"/>
        <attr name="ms_enableErrorLabel" format="boolean"/>
        <attr name="ms_alwaysShowFloatingLabel" format="boolean"/>
        <attr name="ms_isRtl" format="boolean"/>
        <attr name="ms_hintView" format="reference"/>
        <attr name="ms_dropDownHintView" format="reference"/>
    </declare-styleable>

    <item name="visible" type="id" />
    <item name="masked" type="id" />
</resources>

